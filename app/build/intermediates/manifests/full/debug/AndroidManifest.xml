<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="Ex11_Calendar.org"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="9" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="schedulePlanner.org.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="schedulePlanner.org.Daylist"
            android:label="@string/day" >
        </activity>
        <activity
            android:name="schedulePlanner.org.Crud"
            android:label="@string/crud" >
        </activity>
        <activity
            android:name="schedulePlanner.org.Grouplist"
            android:label="@string/group" />
    </application>

</manifest>